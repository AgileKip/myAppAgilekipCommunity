<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!--
        Added the entity Order.
    -->
    <changeSet id="20210830065629-1" author="jhipster">
        <createTable tableName="jhi_order">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="number" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="street_address" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="postal_code" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="city" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="state_province" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="delivery_method" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="cc_number" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="cc_date" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="cc_verifier_digit" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="total" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="user_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="store_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
    </changeSet>

    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->
</databaseChangeLog>
